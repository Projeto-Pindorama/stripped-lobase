ifndef _CONFIG_MK
_CONFIG_MK=1
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
sbindir=@sbindir@
libdir=@libdir@
libexecdir=@libexecdir@
datadir=@datadir@
datarootdir=@datarootdir@
mandir=@mandir@
sysconfdir=@sysconfdir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@

AR=@AR@
CC=@CC@
HOSTCC=@HOSTCC@
INSTALL=@INSTALL@
LD=@LD@
LEX=@LEX@
RANLIB=@RANLIB@
YACC=@YACC@

CFLAGS+=@CFLAGS@
CPPFLAGS+=@CPPFLAGS@ @DEFS@ $(PATHS) -I$(TOPDIR)/include
LDFLAGS+=@LDFLAGS@ -L/lib -L/usr/lib -L/usr/ccs/lib -static

HAVE_STRLCAT=@HAVE_STRLCAT@
HAVE_STRLCPY=@HAVE_STRLCPY@
HAVE_FGETLN=@HAVE_FGETLN@
HAVE_GETENTROPY=@HAVE_GETENTROPY@
HAVE_EXPLICIT_BZERO=@HAVE_EXPLICIT_BZERO@
HAVE_REALLOCARRAY=@HAVE_REALLOCARRAY@
HAVE_RECALLOCARRAY=@HAVE_RECALLOCARRAY@
HAVE_SYS_SIGLIST=@HAVE_SYS_SIGLIST@
HAVE_SYS_SIGNAME=@HAVE_SYS_SIGNAME@
endif
